{
  "name": "gsmlg-cms",
  "description": "GSMLG CMS",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache && yarn start",
    "develop": "nodemon --exec \"gatsby develop\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "lint": "eslint . --ext ts --ext tsx",
    "prettier-write": "prettier --write \"src/**/*.js\"",
    "release": "auto shipit",
    "start": "yarn develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "type-check": "tsc --pretty --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn prettier-write",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "prettier": {
    "bracketSpacing": true,
    "proseWrap": "always",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/rebass": "^4.0.5",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "auto-release-cli": "2.10.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-react": "^7.19.0",
    "gatsby-plugin-typescript": "^2.3.1",
    "graphql-typescript-definitions": "^0.18.1",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "multer": "^1.4.2",
    "netlify-cms-backend-fs": "^0.4.4",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3",
    "typescript-styled-plugin": "^0.15.0",
    "webpack-notifier": "^1.8.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "@mdx-js/tag": "^0.20.3",
    "@reach/router": "^1.3.3",
    "babel-plugin-styled-components": "^1.10.7",
    "d3-ease": "^1.0.6",
    "docz-utils": "^2.3.0",
    "gatsby": "^2.20.15",
    "gatsby-image": "^2.3.2",
    "gatsby-plugin-catch-links": "^2.2.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-mdx": "^1.1.6",
    "gatsby-plugin-netlify-cms": "^4.2.2",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-sharp": "^2.4.4",
    "hast-util-to-string": "^1.0.3",
    "lodash": "^4.17.15",
    "marked": "^0.8.2",
    "netlify-cms": "^2.10.42",
    "netlify-cms-widget-mdx": "^0.4.3",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-head": "^3.3.0",
    "react-powerplug": "^1.0.0",
    "rebass": "^4.0.7",
    "styled-components": "^5.1.0",
    "styled-system": "^5.1.5",
    "unstated": "^2.1.1",
    "write": "^2.0.0"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "authors": [
    "gsmlg <me@gsmlg.org>"
  ],
  "keywords": [
    "Gatsby",
    "TypeScript",
    "Styled Components",
    "Netlify",
    "CMS",
    "MDX",
    "Rebass",
    "Starter"
  ]
}
